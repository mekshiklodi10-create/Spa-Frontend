{
  "name": "create-tailwind-app",
  "version": "1.0.1",
  "description": "Build the HTML template with tailwind css.",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "tailwind": "tailwindcss  -i css/main.css -o public/assets/css/build/tailwind.css --minify  & live-server ./public ",
    "dev": " live-server ./public & postcss css/main.css -o public/assets/css/build/tailwind.css --minify --watch  "
  },
  "keywords": [
    "HTML template",
    "tailwindcss",
    "tailwind",
    "HTML",
    " css framework"
  ],
  "author": "Rajdeep Singh",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "live-server": "^1.2.2",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1",
    "url": "^0.11.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
